SELECT COUNT(1) 笔数,SUM(MONEY) 价格
  FROM LCINSUREACCTRACE A
 WHERE A.MAKEDATE = DATE '2016-09-21'
   AND A.MONEYTYPE IN ('BF', 'AM', 'TB')
   AND A.CONTNO IN (SELECT CONTNO
                      FROM LCCONT B
                     WHERE B.CONTTYPE = '1'
                       AND B.APPFLAG IN ('1', '4')
                       AND B.MANAGECOM LIKE '8647%');
                       
SELECT A.CONTNO,A.MANAGECOM,A.STATE,A.MONEY
  FROM LCINSUREACCTRACE A
 WHERE A.MAKEDATE = DATE '2016-09-21'
   AND A.MONEYTYPE IN ('BF', 'AM', 'TB')
   AND A.CONTNO IN (SELECT CONTNO
                      FROM LCCONT B
                     WHERE B.CONTTYPE = '1'
                       AND B.APPFLAG IN ('1', '4')
                       AND B.MANAGECOM LIKE '8647%');

                       
SELECT COUNT(1) 笔数,SUM(A.APPLY_AMOUNT) 价格
  FROM DEV_PAS.T_FUND_TRANS_APPLY A --投连交易申请表
  LEFT JOIN DEV_PAS.T_CONTRACT_MASTER B
       ON A.POLICY_ID = B.POLICY_ID
 WHERE A.FUND_PROCESS_STATUS = '1' --交易处理状态
   AND B.ORGAN_CODE LIKE '8647%' --机构代码
   AND TO_CHAR(A.UPDATE_TIME, 'YYYY-MM-DD') = '2016-09-21';
   
SELECT DISTINCT B.POLICY_CODE,B.ORGAN_CODE,A.FUND_PROCESS_STATUS,A.APPLY_AMOUNT
  FROM DEV_PAS.T_FUND_TRANS_APPLY A --投连交易申请表
  LEFT JOIN DEV_PAS.T_CONTRACT_MASTER B
       ON A.POLICY_ID = B.POLICY_ID
 WHERE A.FUND_PROCESS_STATUS = '1' --交易处理状态
   AND B.ORGAN_CODE LIKE '8647%' --机构代码
   AND TO_CHAR(A.UPDATE_TIME, 'YYYY-MM-DD') = '2016-09-21';
